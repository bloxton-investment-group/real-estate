{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "src/app/api/process-pdf/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/update-bill-pages/route.ts": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/pdf.worker.min.mjs",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ]
}